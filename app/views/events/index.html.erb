<section class="inner-banner"> <img src="images/citybg.jpg" alt="">
  <section class="inner-banner-text">
    <h1><strong>Omecto</strong> Connects People Locally</h1>
    <p>Omecto is a platform for creating new connections anytime, anywhere in a formal or informal way. People use Omecto to host & join events,reach targeted audiences, meet new people, find support, solve a problem and pursue their passions, together.</p>
    <a href="#" class="joinbtn">Join Omecto</a> </section>
</section>
<section class="container">
  <section class="search-sec">
    <section class="search-sec1">
      <div class="row">
        <div class="col-md-4">
          <div class="search-div">
            <form name="searchform" method="post" action="#">
              <input name="search" type="text" placeholder="search" required>
              <input name="submit" type="submit" value="">
            </form>
          </div>
        </div>
        <div class="col-md-4 search-text">within 50 miles of Kolkata, IN</div>
        <div class="col-md-4">
          <div class="col-md-6 padd-0"><a href="#" class="groups-btn">Groups</a></div>
          <div class="col-md-6 padd-0"><a href="#" class="calendar-btn">Calendar</a></div>
        </div>
      </div>
    </section>
  </section>
</section>
<section class="explore-city">
  <section class="container">
    <div class="row">
      <div class="col-md-6">
        <h3><a href="#">sort by best match</a></h3>
      </div>
      <div class="col-md-6 text-right">
        <h3><a href="#">explore more <img src="images/arrow1.png" alt=""></a></h3>
      </div>
    </div>
    <section class="events-cate">
      <ul class="row categories-all">
        <% @events.each do |event| %>
          <li class="col-lg-4 col-md-4 col-sm-6">
            <div class="cat-wrap">
              <a href="" data-toggle="modal" data-target="#get-offer">
                <img src="/images/city1.jpg" alt="">
                <a class="zoom1" href="">
                  <img src="/images/zoom.png" alt="">
                </a>
                <div class="hover1">
                  <a href="#">
                    <%= event.title %>
                  </a>5,585 members
                </div>
              </a>
            </div>
          </li>
        <% end %>
      </ul>
    </section>
  </section>
</section>
<div class="modal events-popup" id="get-offer" role="dialog" style="display: none;">
  <div class="modal-dialog new-modal">
    <div class="modal-content">
      <div class="modal-header no-border">
        <button type="button" class="close" data-dismiss="modal">×</button>
      </div>
      <div class="modal-body">
        <div class="row">
          <div class="col-md-5"><img src="images/event1.png" alt=""></div>
          <div class="col-md-7">
            <h3>Events Schedule</h3>
            <h4>sponsor club kim</h4>
            <h5>hosted by dextor</h5>
            <p style="color:#000;">Michael Lambert</p>
            <p class="greytext">Lorem ipsum dolor sit amet, consectetur adipisc elit. Proin ultricies vestibulum velit.</p>
            <p><i class="fa fa-clock-o" aria-hidden="true"></i> 7pm-11pm</p>
            <p><i class="fa fa-calendar-check-o" aria-hidden="true"></i> 03-Jul-2019</p>
            <p class="text-uppercase"><i class="fa fa-map-marker" aria-hidden="true"></i> Kolkata Nicco Park</p>
            <div class="row marg-top-10">
              <div class="col-md-6 text-center"><a href="#"><img src="images/join.jpg" alt=""></a></div>
              <div class="col-md-6"><a href="#" class="cancel-btn">Cancel</a></div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
