<section class="section-signup">
  <div class="container">
    <div class="signup-container">
      <div class="signup-container1">
        <h3>Sign Up</h3>
        <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
          <%= render "devise/shared/error_messages", resource: resource %>
          <%= f.email_field :email, autofocus: true, autocomplete: "email", placeholder: "Email" %>
          <%= f.password_field :password, autocomplete: "new-password", placeholder: "Password" %>
          <%= f.password_field :password_confirmation, autocomplete: "new-password", placeholder: "Password confirmation" %>
          <%= f.submit "Sign up" %>
        <% end %>
        <p>- Or Sign In With  - </p>
        <div class="row">
          <div class="col-lg-6"><a href="#" class="fb-button"><i class="fa fa-facebook" aria-hidden="true"></i> Facebook</a></div>
          <div class="col-lg-6"><a href="#" class="fb-button"><i class="fa fa-google-plus" aria-hidden="true"></i> Google Plus</a></div>
        </div>
        <p>By creating an account, you agree to our <a href="#">terms</a>.</p>
      </div>
    </div>
  </div>
</section>
